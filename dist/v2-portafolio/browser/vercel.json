{
    "version": 2,
    "builds": [
      {
        "src": "dist/v2-portafolio/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(assets/.+)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" }
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }